      "source": [
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "print(\"for\")\n",
        "for i in array:\n",
        "  print(i)\n",
        "\n",
        "#要素数の取得\n",
        "element_count = len(array)\n",
        "\n",
        "#ループ変数の定義\n",
        "index = 0\n",
        "\n",
        "print(\"while\")\n",
        "while index < element_count:\n",
        "  print(array[index])\n",
        "  index = index + 1\n"
      ]
